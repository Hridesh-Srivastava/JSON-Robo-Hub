{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "views/**/*",
            "public/**/*"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/styles/(.*)",
        "dest": "/public/styles/$1"
      },
      {
        "src": "/(.+\\.png)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["public/**", "views/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/styles/(.*)",
        "dest": "/public/styles/$1"
      },
      {
        "src": "/logo.png",
        "dest": "/public/logo.png"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }